<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.imp.runtime.languageDescription">
      <language
            extensions="aj,java"
            description="ejp"
            language="ejp"
            derivedFrom="DynamicRoot"
            validator="EJP.EjpValidator"
            validatorClass="EJP.EjpValidator"
      ></language>
   </extension>
   <extension
         id="EJP.parser"
         name="ejp Parser"
         point="org.eclipse.imp.runtime.parser">
      <parser
            class="EJP.EjpParseController"
            language="ejp">
      </parser>
   </extension>    
</plugin>
