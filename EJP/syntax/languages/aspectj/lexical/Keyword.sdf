module languages/aspectj/lexical/Keyword
exports
  lexical restrictions
    "privileged"
    "aspect"
    "after"
    "around"
    "before"
    "declare"
    "issingleton"
    "percflow"
    "percflowbelow"
    "pertarget"
    "perthis"
    "pointcut"
      -/- [A-Za-z0-9\_\$]
